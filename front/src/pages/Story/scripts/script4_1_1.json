    "LeftImageUrl2": "a",
    "RightImageUrl3": "a",
    "backgroundImageUrl": "classroom",
    "plusIndex": 1,
		"failPlusIndex": 56
  },
	{
    "idx": 36,
    "scriptType": "text",
    "storyTeller": "서봄",
    "text": "\"너와 함께 회사를 다니고 싶기 때문이야.\"",

    "LeftImageUrl2": "a",
    "RightImageUrl3": "a",
    "backgroundImageUrl": "classroom",
    "plusIndex": 1
  },
	{
    "idx": 4,
    "scriptType": "text",
    "storyTeller": "서상균",
